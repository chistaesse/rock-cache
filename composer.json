    "keywords": ["caching", "race-condition", "dog-pile", "tagging-cache", "redis", "memcache", "memcached", "apc", "memory", "couchbase"],
    "type": "library",
    "license": "MIT",
    "support": {
        "email": "serggalka@gmail.com",
        "issues": "https://github.com/romao7/rock-cache/issues",
        "source": "https://github.com/romao7/rock-cache"
    },
    "authors": [
        {
            "name": "Ruslan",
            "email": "serggalka@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "league/flysystem": "0.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-4": {
            "rock\\cache\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}